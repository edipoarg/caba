# NocoDB credentials
# Copy this file to .env and fill in your values
NOCODB_BASE_URL=https://your-nocodb-instance.example.com
NOCODB_API_KEY=your_api_key_here

# Optional: request tuning
# Timeout per HTTP request in milliseconds (default 60000)
NOCODB_TIMEOUT_MS=60000
# Max automatic retries for transient errors/timeouts (default 3)
NOCODB_MAX_RETRIES=3
# Base delay for exponential backoff in milliseconds (default 1000)
NOCODB_RETRY_BASE_DELAY_MS=1000
